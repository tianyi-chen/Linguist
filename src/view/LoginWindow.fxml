<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" styleClass="background" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label fx:id="usernameLabel" layoutX="89.0" layoutY="85.0" stylesheets="@application.css" text="username:" />
      <Label fx:id="passwordLabel" layoutX="89.0" layoutY="147.0" text="password:" />
      <Label fx:id="usernameRequireLabel" layoutX="205.0" layoutY="118.0" />
      <Label fx:id="passwordRequireLabel" layoutX="205.0" layoutY="180.0" />
      <Label fx:id="loginLabel" layoutX="144.0" layoutY="43.0" />
      <TextField fx:id="usernameText" layoutX="202.0" layoutY="78.0" />
      <PasswordField fx:id="passwordField" layoutX="202.0" layoutY="140.0" onKeyPressed="#buttonPressed" />
      <HBox layoutX="72.0" layoutY="224.0" spacing="200.0">
         <children>
            <Button fx:id="signUpButton" mnemonicParsing="false" onAction="#onClickSignUp" prefHeight="28.0" prefWidth="96.0" text="SignUp" />
            <Button fx:id="OKButton" mnemonicParsing="false" onAction="#onClickOK" prefHeight="32.0" prefWidth="72.0" text="OK" />
         </children>
      </HBox>
   </children>
</AnchorPane>
