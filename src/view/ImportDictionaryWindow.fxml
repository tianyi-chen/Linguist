<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" styleClass="background" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DictionaryController">
   <children>
      <HBox layoutX="76.0" layoutY="233.0" spacing="200.0">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onClickCancel" text="CANCEL" />
            <Button fx:id="OKButtonforImport" mnemonicParsing="false" onAction="#onClickOKforImport" text="OK" />
         </children>
      </HBox>
      <Label fx:id="pathLabel" layoutX="53.0" layoutY="70.0" text="file path" />
      <Label fx:id="dictionaryNameLabel" layoutX="51.0" layoutY="157.0" text="dictionary name" />
      <TextField fx:id="filePathText" layoutX="207.0" layoutY="63.0" prefHeight="32.0" prefWidth="240.0" />
      <TextField fx:id="dictionaryNameText" layoutX="209.0" layoutY="151.0" onKeyPressed="#enterPressedforImport" prefHeight="32.0" prefWidth="240.0" />
      <Label fx:id="filePathRequireLabel" layoutX="209.0" layoutY="102.0" />
      <Label fx:id="dictionaryNameRequireLabel" layoutX="209.0" layoutY="190.0" />
      <Button fx:id="chooseFile" layoutX="112.0" layoutY="66.0" mnemonicParsing="false" onAction="#onClickChooseFileButton" text="Browse" />
   </children>
</AnchorPane>