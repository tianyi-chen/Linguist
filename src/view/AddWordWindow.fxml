<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" styleClass="background" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DictionaryController">
   <children>
      <Label fx:id="wordLabel" layoutX="85.0" layoutY="32.0" text="word:" />
      <Label fx:id="wordClassLabel" layoutX="85.0" layoutY="89.0" text="word class:" />
      <Label fx:id="meaningLabel" layoutX="85.0" layoutY="146.0" text="meaning:" />
      <Label fx:id="exampleLabel" layoutX="85.0" layoutY="203.0" text="example:" />
      <TextField fx:id="wordText" layoutX="206.0" layoutY="22.0" />
      <TextField fx:id="wordClassText" layoutX="206.0" layoutY="77.0" />
      <TextField fx:id="meaningText" layoutX="206.0" layoutY="132.0" />
      <TextField fx:id="exampleText" layoutX="206.0" layoutY="187.0" onKeyPressed="#enterPressedforAddWord" />
      <HBox layoutX="98.0" layoutY="244.0" spacing="150.0">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onClickCancel" prefHeight="32.0" prefWidth="103.0" text="CANCEL" />
            <Button fx:id="OKButtonforAddWord" mnemonicParsing="false" onAction="#onClickOKforAddWord" prefWidth="65.0" text="OK" />
         </children>
      </HBox>
      <Label fx:id="contentRequireLabel" layoutX="206.0" layoutY="57.0" />
      <Label fx:id="classRequireLabel" layoutX="206.0" layoutY="112.0" />
      <Label fx:id="meaningRequireLabel" layoutX="206.0" layoutY="167.0" />
      <Label fx:id="exampleRequireLabel" layoutX="206.0" layoutY="222.0" />
   </children>
</AnchorPane>